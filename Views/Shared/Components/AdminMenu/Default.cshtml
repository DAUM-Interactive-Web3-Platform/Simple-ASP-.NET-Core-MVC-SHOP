@model IEnumerable<MyShop.Components.AdminMenuItem>

<li class="dropdown">
    <a asp-controller="Admin" asp-action="Index" class="dropdown-toggle" data-toggle="dropdown">
        Admin<b class="caret"></b>
    </a>
    <ul class="dropdown-menu">
        @foreach(var item in Model)
        {
            <li>
                <a asp-controller="Admin" asp-action="@item.ActionValue">@item.DisplayValue</a>
            </li>
        }
    </ul>
</li>